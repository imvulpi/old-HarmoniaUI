[configuration]

entry_symbol = "harmonia_init"
compatibility_minimum = "4.1"

[libraries]

macos.debug = "./bin/harmoniaui.macos.template_debug.framework"
macos.release = "./bin/harmoniaui.macos.template_release.framework"
windows.debug.x86_32 = "./bin/harmoniaui.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "./bin/harmoniaui.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "./bin/harmoniaui.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "./bin/harmoniaui.windows.template_release.x86_64.dll"
windows.debug.arm32 = "./bin/harmoniaui.windows.template_debug.arm32.dll"
windows.release.arm32 = "./bin/harmoniaui.windows.template_release.arm32.dll"
windows.debug.arm64 = "./bin/harmoniaui.windows.template_debug.arm64.dll"
windows.release.arm64 = "./bin/harmoniaui.windows.template_release.arm64.dll"
linux.debug.x86_32 = "./bin/libharmoniaui.linux.template_debug.x86_32.so"
linux.release.x86_32 = "./bin/libharmoniaui.linux.template_release.x86_32.so"
linux.debug.x86_64 = "./bin/libharmoniaui.linux.template_debug.x86_64.so"
linux.release.x86_64 = "./bin/libharmoniaui.linux.template_release.x86_64.so"
linux.debug.arm32 = "./bin/libharmoniaui.linux.template_debug.arm32.so"
linux.release.arm32 = "./bin/libharmoniaui.linux.template_release.arm32.so"
linux.debug.arm64 = "./bin/libharmoniaui.linux.template_debug.arm64.so"
linux.release.arm64 = "./bin/libharmoniaui.linux.template_release.arm64.so"
linux.debug.rv64 = "./bin/libharmoniaui.linux.template_debug.rv64.so"
linux.release.rv64 = "./bin/libharmoniaui.linux.template_release.rv64.so"
android.debug.x86_32 = "./bin/libharmoniaui.android.template_debug.x86_32.so"
android.release.x86_32 = "./bin/libharmoniaui.android.template_release.x86_32.so"
android.debug.x86_64 = "./bin/libharmoniaui.android.template_debug.x86_64.so"
android.release.x86_64 = "./bin/libharmoniaui.android.template_release.x86_64.so"
android.debug.arm32 = "./bin/libharmoniaui.android.template_debug.arm32.so"
android.release.arm32 = "./bin/libharmoniaui.android.template_release.arm32.so"
android.debug.arm64 = "./bin/libharmoniaui.android.template_debug.arm64.so"
android.release.arm64 = "./bin/libharmoniaui.android.template_release.arm64.so"
ios.debug = "./bin/harmoniaui.ios.template_debug.xcframework"
ios.release = "./bin/harmoniaui.ios.template_release.xcframework"
web.debug.threads.wasm32 = "./bin/harmoniaui.web.template_debug.wasm32.wasm"
web.release.threads.wasm32 = "./bin/harmoniaui.web.template_release.wasm32.wasm"

[dependencies]
ios.debug = {
  "./bin/libgodot-cpp.ios.template_debug.xcframework": ""
}
ios.release = {
  "./bin/libgodot-cpp.ios.template_release.xcframework": ""
}
